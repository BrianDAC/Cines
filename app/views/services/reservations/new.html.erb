<%= form_for @reservation, url: services_reservations_create_path do |r| %>
      <%= hidden_field_tag "show_id", @show.id %>
      <%= hidden_field_tag "show_sala", @show.room.number %>
      <% Showseat.where(:show_id => @show.id).where(:status => 2).find_each do |ocu| %>
          <% @ocupados.push(ocu.seat.seat) %>
      <% end %>
