            </div>
          </div>

          <div class="col-sm-5">
            <div class="form-group">
              <%= label_tag( :ci, "Cedula") %> <br>
              <%= r.text_field :ci, :class => 'form-control', :placeholder => "Cedula" %>
            </div>
            <%= r.hidden_field :show_id, :value => @show.id %>
            <div class="form-group">
              <%= label_tag( :elderly, "Mayores de 65 años") %> <br>
